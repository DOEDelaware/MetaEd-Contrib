  </xs:annotation>
  <xs:complexType name="EXTENSION-StudentCTEProgramAssociationExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Association</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="StudentCTEProgramAssociation">
        <xs:sequence>
          <xs:element name="CertificatedProgramStatus" type="EXTENSION-CertificatedProgramStatusDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicator of Certificated Program Status</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-CertificatedProgramStatusDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="StateEndorsedRegionalCareerPathwayStatus" type="EXTENSION-StateEndorsedRegionalCareerPathwayStatusDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicator of StateEndorsedRegionalCareerPathwayStatus</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-StateEndorsedRegionalCareerPathwayStatusDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="CTEConcentrationCteProgramArea" type="EXTENSION-CteProgramAreaDescriptorReferenceType" minOccurs="0" maxOccurs="unbounded">
