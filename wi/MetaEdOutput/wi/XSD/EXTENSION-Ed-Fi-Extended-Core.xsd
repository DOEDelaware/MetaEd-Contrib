<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://ed-fi.org/0320b" xmlns:ann="http://ed-fi.org/annotation" targetNamespace="http://ed-fi.org/0320b" elementFormDefault="qualified" attributeFormDefault="unqualified">
  <xs:include schemaLocation="Ed-Fi-Core.xsd" />
  <xs:annotation>
    <xs:documentation>===== Ed-Fi 3.2.0-b Extensions =====</xs:documentation>
  </xs:annotation>
  <xs:annotation>
    <xs:documentation>===== Domain Entities =====</xs:documentation>
  </xs:annotation>
  <xs:complexType name="EXTENSION-CourseOfferingExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="CourseOffering">
        <xs:sequence>
          <xs:element name="CareerPathway" type="CareerPathwayDescriptorReferenceType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>The career cluster or pathway representing the career path of the Vocational/Career Tech concentrator.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>CareerPathwayDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-DisciplineActionExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="DisciplineAction">
        <xs:sequence>
          <xs:element name="ModifiedTerm" type="EXTENSION-ModifiedTermDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Identifies if the duration of the expulsion has been modified from the federally required one year of expulsion for a fire arms incident.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-ModifiedTermDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="EarlyReinstatementCondition" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Following an expulsion, the student had early reinstatement condition to return to school prior to the end of the expulsion.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-GradeExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="Grade">
        <xs:sequence>
          <xs:element name="CertificatedProgramStatus" type="EXTENSION-CertificatedProgramStatusDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicator of Certificated Program Status</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-CertificatedProgramStatusDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-GraduationPlanExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="GraduationPlan">
        <xs:sequence>
          <xs:element name="CommunityService" type="EXTENSION-CommunityService">
            <xs:annotation>
              <xs:documentation>Total hours of community service required for graduation</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CivicsExam" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>A civics exam is required- Yes -1 or No - 0</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="MiscellaneousRequiredCredits" type="EXTENSION-MiscellaneousRequiredCredits" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Clarification of requirements for Miscellaneous Required Credits</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="NonHsHealthEducationCredits" type="CreditsValue">
            <xs:annotation>
              <xs:documentation>Credits of Health Education Fulfilled in Grade 7 and/or Grade 8</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
