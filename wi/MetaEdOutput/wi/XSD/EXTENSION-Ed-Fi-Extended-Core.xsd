        <ann:TypeGroup>Common</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="CountDateName" type="EXTENSION-CountDateNameDescriptorReferenceType">
        <xs:annotation>
          <xs:documentation>The count date name for the student status (e.g 3rd Fri Sept)</xs:documentation>
          <xs:appinfo>
            <ann:Descriptor>EXTENSION-CountDateNameDescriptor</ann:Descriptor>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element name="CountDateReceivingService" type="EXTENSION-CountDateReceivingServiceDescriptorReferenceType">
        <xs:annotation>
          <xs:documentation>The status of the student during the specific count date (e.g. Present)</xs:documentation>
          <xs:appinfo>
            <ann:Descriptor>EXTENSION-CountDateReceivingServiceDescriptor</ann:Descriptor>
          </xs:appinfo>
        </xs:annotation>
