              <xs:documentation></xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-StudentSchoolAssociationExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Association</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="StudentSchoolAssociation">
        <xs:sequence>
          <xs:element name="CompletedSchoolTerm" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation></xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ExpectedTransferLocalEducationAgencyReference" type="LocalEducationAgencyReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation></xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ExpectedTransferSchoolReference" type="SchoolReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation></xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ActualDaysAttendance" type="EXTENSION-ActualDaysAttendance" minOccurs="0">
            <xs:annotation>
              <xs:documentation></xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="PossibleDaysAttendance" type="EXTENSION-PossibleDaysAttendance" minOccurs="0">
            <xs:annotation>
              <xs:documentation></xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="PrivateSchoolChoiceProgramParticipant" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation></xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ReceivingService" type="EXTENSION-ReceivingService" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>The status of a student during a specific count date</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="EnrollmentType" type="EXTENSION-EnrollmentTypeDescriptorReferenceType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation></xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-EnrollmentTypeDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
