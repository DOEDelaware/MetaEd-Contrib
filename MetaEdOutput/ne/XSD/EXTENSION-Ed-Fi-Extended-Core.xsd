            </xs:annotation>
          </xs:element>
          <xs:element name="GunFreeSchoolViolation" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation>True = Student was charged with a Gun-Free Schools Violation.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SchoolRelatedArrestIndicator" type="xs:boolean" minOccurs="0">
