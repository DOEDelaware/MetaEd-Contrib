<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://ed-fi.org/0300" xmlns:ann="http://ed-fi.org/annotation" targetNamespace="http://ed-fi.org/0300" elementFormDefault="qualified" attributeFormDefault="unqualified">
  <xs:include schemaLocation="Ed-Fi-Core.xsd" />
  <xs:annotation>
    <xs:documentation>===== Ed-Fi 3.0.0 Extensions =====</xs:documentation>
  </xs:annotation>
  <xs:annotation>
    <xs:documentation>===== Domain Entities =====</xs:documentation>
  </xs:annotation>
  <xs:complexType name="EXTENSION-CalendarDateTrackEvent">
    <xs:annotation>
      <xs:documentation>The calendar track event associated with a calendar date.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="Date" type="xs:date">
            <xs:annotation>
              <xs:documentation>The month, day, and year of the CalendarDate.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="TrackReference" type="EXTENSION-TrackReferenceType">
            <xs:annotation>
              <xs:documentation>The track reference</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="TrackEvent" type="EXTENSION-TrackEventDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>The type of scheduled or unscheduled event for the day. For example:
              
                                  Instructional day
              
                                  Teacher only day
              
                                  Holiday
              
                                  Make-up day
              
                                  Weather day
              
                                  Student late arrival/early dismissal.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-TrackEventDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="EventDuration" type="EventDuration">
            <xs:annotation>
              <xs:documentation>The amount of time for the event as recognized by the school: 1 day = 1,
                                  1/2 day = 0.5, 1/3 day = 0.33.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-CourseOfferingExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="CourseOffering">
        <xs:sequence>
          <xs:element name="EndOfCourseAssessmentCode" type="EXTENSION-EndOfCourseAssessmentCodeDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The end of course assesment code. Submitted for courses that end with Assessment testing</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-EndOfCourseAssessmentCodeDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="CourseOfferingCourseSchedule" type="EXTENSION-CourseOfferingCourseSchedule" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>CourseSchedule for given course</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-SectionExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="Section">
        <xs:sequence>
          <xs:element name="SectionCourseCharacteristic" type="EXTENSION-SectionCourseCharacteristic" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>A collection of course level characteristics for a section</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SectionExternalProviderTeacher" type="EXTENSION-SectionExternalProviderTeacher" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>A collection fo external teachers assigned to a section</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SectionGradeLevel" type="EXTENSION-SectionGradeLevel" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>The grade level associated with the section.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="TimetableDayIdentifier" type="EXTENSION-TimetableDayIdentifier" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Report the days on which the course/section meets. MTWRF if meets
                              every day, MWF if the course/section meets every other day. If
                              the course is on a block schedule, use AB.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Note" type="EXTENSION-Note" minOccurs="0">
            <xs:annotation>
              <xs:documentation>This is a free field that may contain up to 250 characters.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-SessionExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="Session">
        <xs:sequence>
          <xs:element name="SessionType" type="EXTENSION-SessionTypeDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The Session Type is a code indicating whether the session in which
                              the course is conducted is a Semester, Trimester, Quarter, Full
                              Year. Can be thought of as 'term definition'.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-SessionTypeDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-StudentDropOutRecoveryProgramMonthlyUpdate">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="StudentDropOutRecoveryProgramAssociationReference" type="EXTENSION-StudentDropOutRecoveryProgramAssociationReferenceType">
            <xs:annotation>
              <xs:documentation>The drop out recovery program that the monthly update is reported</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="RevisedWrittenLearningPlanDate" type="xs:date" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The Revised Learning Plan Date is used when plan requires changes
                                to  the initial written plan.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Month" type="EXTENSION-MonthDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>the month for which the progress update is reported</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-MonthDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="SatisfactoryProgress" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates whether student maintained satisfactory progress during
                                the month prior to this record. This data must be submitted each
                                month that the student is enrolled in the DRP regardless of the
                                student's progress. This field is nullable.  If no data is submitted,
                                it will be NULL.  If the LEA/School submits data it will be a
                                0 or a 1 where 0 = Unsatisfactory and 1 = Satisfactory</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-StudentExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="Student">
        <xs:sequence>
          <xs:element name="TribalName" type="EXTENSION-TribalName" minOccurs="0">
            <xs:annotation>
              <xs:documentation>A name borne in common by members of a tribe or clan. This  is
                                used for matching purposes only. Note: Name provided by Tribe,
                                not name of Tribe.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-StudentNeed">
    <xs:annotation>
      <xs:documentation>This entity represents a need for a student</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="StudentReference" type="StudentReferenceType">
            <xs:annotation>
              <xs:documentation>A unique alphanumeric code assigned to a student</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="EducationOrganizationReference" type="EducationOrganizationReferenceType">
            <xs:annotation>
              <xs:documentation>The identifier assigned to an education agency reporting the need</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="StudentNeed" type="EXTENSION-StudentNeedDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>The student need descriptor code</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-StudentNeedDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="StudentNeedEntryDate" type="xs:date">
            <xs:annotation>
              <xs:documentation>The date the student began the state of having the Need.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="StudentNeedExitDate" type="xs:date" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The date the student exited the state of having the Need.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="PrimaryStudentNeedIndicator" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates if the SPED Need being serviced is the Primary Need.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="PrimaryNightTimeResidence" type="EXTENSION-PrimaryNightTimeResidenceDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The Primary Night time residence descriptor associated with student
                                who have been designated as Homeless.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-PrimaryNightTimeResidenceDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-StudentProgramAttendanceEventExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="StudentProgramAttendanceEvent">
        <xs:sequence>
          <xs:element name="TimeLog" type="EXTENSION-TimeLog" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>This event entity adds the time component to a student program attendance participating</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-StudentSchoolAssociationLocalEducationAgency">
    <xs:annotation>
      <xs:documentation>This association indicates any relationship between a student school association and a Local Education Agency</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="StudentSchoolAssociationReference" type="StudentSchoolAssociationReferenceType">
            <xs:annotation>
              <xs:documentation>The reference to StudentSchoolAssociation</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="StartDate" type="xs:date">
            <xs:annotation>
              <xs:documentation>The start date of the association</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="EndDate" type="xs:date" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The end date of the association</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="LocalEducationAgencyReference" type="LocalEducationAgencyReferenceType">
            <xs:annotation>
              <xs:documentation>The reference to the LocalEducationAgency</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="MembershipResponsibility" type="EXTENSION-MembershipResponsibilityDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>The membership responsibility descriptor</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-MembershipResponsibilityDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-StudentSchoolAssociationMembershipFTE">
    <xs:annotation>
      <xs:documentation>This association represents the full-time equivalency of the student school association</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="StudentSchoolAssociationReference" type="StudentSchoolAssociationReferenceType">
            <xs:annotation>
              <xs:documentation>The reference to StudentSchoolAssociation</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="FTEStartDate" type="xs:date">
            <xs:annotation>
              <xs:documentation>The start date for the StudentSchoolAssociationMembership</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="FTEEndDate" type="xs:date" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The end date for the StudentSchoolAssociationMembership</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="MembershipFTE" type="EXTENSION-MembershipFTEDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>The membership full-time equivalency descriptor</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-MembershipFTEDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-StudentSchoolAssociationSpecialEnrollment">
    <xs:annotation>
      <xs:documentation>This association represents any special enrollment characteristics of the student school association</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="StudentSchoolAssociationReference" type="StudentSchoolAssociationReferenceType">
            <xs:annotation>
              <xs:documentation>The reference to StudentSchoolAssociation</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SpecialEnrollmentStartDate" type="xs:date">
            <xs:annotation>
              <xs:documentation>The start date for the StudentSchoolAssociationSpecialEnrollment</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SpecialEnrollmentEndDate" type="xs:date" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The end date for the StudentSchoolAssociationSpecialEnrollment</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SpecialEnrollment" type="EXTENSION-SpecialEnrollmentDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>The special enrollment descriptor</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-SpecialEnrollmentDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-StudentSchoolAssociationTuitionPayer">
    <xs:annotation>
      <xs:documentation>This association represents the tuition payer codes of the student school association</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="StudentSchoolAssociationReference" type="StudentSchoolAssociationReferenceType">
            <xs:annotation>
              <xs:documentation></xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="PayerStartDate" type="xs:date">
            <xs:annotation>
              <xs:documentation>The start date for the StudentSchoolAssociationTuitionPayer</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="PayerEndDate" type="xs:date" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The end date for the StudentSchoolAssociationTuitionPayer</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="TuitionPayer" type="EXTENSION-TuitionPayerDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>The Tuition Payer descriptor</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-TuitionPayerDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-StudentSchoolAttendanceEventExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="StudentSchoolAttendanceEvent">
        <xs:sequence>
          <xs:element name="AbsenceAmount" type="EXTENSION-AbsenceAmountDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Absence Amount descriptor. Absence Amounts must be less than or equal to the  Student Membership FTE, since absences are measured in quarter day increments.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-AbsenceAmountDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="InstructionalMinutes" type="xs:int" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The amount of time, in minutes, that the student received instruction during the reported attendance period and the 'In Attendance' code is used</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-Track">
    <xs:annotation>
      <xs:documentation>This represents a calendar track for a school</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="TrackSchoolIdSchoolReference" type="SchoolReferenceType">
            <xs:annotation>
              <xs:documentation>OrganizationID of the School which is tied to the track</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="TrackNumber" type="EXTENSION-TrackNumberDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>The track number descriptor (e.g. 01, 02, 03 etc.)</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-TrackNumberDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="TrackLocalEducationAgencyReference" type="LocalEducationAgencyReferenceType">
            <xs:annotation>
              <xs:documentation>The LEA id of the district submitting the track</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="BeginDate" type="xs:date">
            <xs:annotation>
              <xs:documentation>The first date of the track</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="EndDate" type="xs:date">
            <xs:annotation>
              <xs:documentation>The last date of the track</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="TotalInstructionalDays" type="TotalInstructionalDays">
            <xs:annotation>
              <xs:documentation>The total number of instructional days in the school calendar.
                                National Education Data Model (NEDM): Total Days in Session</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="NumberOfInstructionalDaysInWeek" type="EXTENSION-NumberOfInstructionalDaysInWeekDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>The number of instructional days in a week descriptor (e.g. 3, 4, 5)</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-NumberOfInstructionalDaysInWeekDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:annotation>
    <xs:documentation>===== Descriptors =====</xs:documentation>
  </xs:annotation>
  <xs:complexType name="EXTENSION-AbsenceAmountDescriptor">
    <xs:annotation>
      <xs:documentation>Absence Amounts must be less than or equal to the  Student Membership
                        FTE, since absences are measured in quarter day increments.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Descriptor</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="DescriptorType" />
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-CourseEntryDescriptor">
    <xs:annotation>
      <xs:documentation>The way the student entered this course, either as a
                          new enrollee or as a transfer from another section at the same
                          school</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Descriptor</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="DescriptorType" />
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-CourseExitDescriptor">
    <xs:annotation>
      <xs:documentation>The type of exit the student had from this course section.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Descriptor</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="DescriptorType" />
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-EndOfCourseAssessmentCodeDescriptor">
    <xs:annotation>
      <xs:documentation>Submitted for courses that end with Assessment testing</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Descriptor</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="DescriptorType" />
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-ExitWithdrawReasonDescriptor">
    <xs:annotation>
      <xs:documentation>Withdrawal Reasons are a secondary reason for the withdrawal (the
                        primary reason being the Withdrawal Activity Code). Withdrawal
                        Reasons are optional, and will not apply to the great majority
                        of students who withdraw from school prior to the last scheduled
                        day of session.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Descriptor</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="DescriptorType" />
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-MembershipFTEDescriptor">
    <xs:annotation>
      <xs:documentation>This descriptor describes the full-time equivalency codes</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Descriptor</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="DescriptorType" />
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-MembershipResponsibilityDescriptor">
    <xs:annotation>
      <xs:documentation>This descriptor defines types of responsibility a local education agency may have for a student school association(e.g., funding).</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Descriptor</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="DescriptorType" />
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-MembershipTypeDescriptor">
    <xs:annotation>
      <xs:documentation>Membership Type identifies whether a school has primary responsibility
                        for managing a specific student's curriculum or not. The 'P' code
                        value will allow the reporting of students who are enrolled in
                        a SPED program but not enrolled in a school.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Descriptor</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
